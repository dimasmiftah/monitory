        </activity>
        <activity android:name="me.dimasmiftah.monitory.PersonalFormActivity" />
        <activity android:name="me.dimasmiftah.monitory.VerifyCodeActivity" />
        <activity android:name="me.dimasmiftah.monitory.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>